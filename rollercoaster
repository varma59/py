def solve(a):
    if not a:
        return 0
    current_answer = 1
    best_answer = 1
    for i in range(1, len(a)):
        if a[i] > a[i - 1]:
            current_answer += 1
        else:
            current_answer = 1
        best_answer = max(best_answer,current_answer)
    return best_answer * 10               
    
    
